-- Initial KMC-haskell.cabal generated by cabal init.  For further 
-- documentation, see http://haskell.org/cabal/users-guide/

name:                KMC-haskell
version:             0.1.0.0
-- synopsis:            
-- description:         
license:             MIT
license-file:        LICENSE
author:              Rachael Oxley
maintainer:          rnoxley@gmail.com
-- copyright:           
category:            Scientific
build-type:          Simple
extra-source-files:  README.md
cabal-version:       >=1.10

executable KMC-haskell
  main-is:             Main.hs
  hs-source-dirs:      src
  ghc-options:         -O2 -rtsopts -fprof-auto
  other-modules:       FIFO
                       , KMC.Config
                       , KMC.Graph
                       , KMC.Lattice
                       , KMC.Reactions
                       , KMC.States
                       , KMC.Types
  other-extensions:    OverloadedStrings
  build-depends:       base  , vector  , heaps  , random  , unordered-containers  , aeson  , text  , time  , bytestring  , hashable  , mtl  , ghc-prim  , vector-algorithms  , primitive  
  default-language:    Haskell2010
